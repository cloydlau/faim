{
  "name": "kikimore",
  "PascalCasedName": "Kikimore",
  "version": "0.13.3",
  "private": false,
  "packageManager": "pnpm@7.25.0",
  "description": "Several components wrapper for Element Plus / Element.",
  "author": "Cloyd Lau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/kikimore",
  "keywords": [
    "vue",
    "vue-demi",
    "element",
    "element-ui",
    "element-plus"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": "./*"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "unpkg": "./src/index.ts",
  "jsdelivr": "./src/index.ts",
  "types": "./src/index.d.ts",
  "files": [
    "docs",
    "src"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "build": "vite build",
    "release": "npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-plus": ">=2",
    "element-ui": ">=2",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    },
    "element-ui": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-vue2": "latest",
    "@vue/test-utils": "1",
    "case-police": "latest",
    "element-ui": "latest",
    "husky": "latest",
    "json-editor-vue": "latest",
    "lint-staged": "latest",
    "rome": "latest",
    "sass": "latest",
    "stylelint": "latest",
    "stylelint-config-recommended-vue": "latest",
    "stylelint-config-standard-scss": "latest",
    "typescript": "latest",
    "unplugin-auto-import": "latest",
    "unplugin-vue-components": "latest",
    "vanilla-jsoneditor": "latest",
    "vite": "latest",
    "vite-plugin-checker": "latest",
    "vite-plugin-dts": "latest",
    "vls": "latest",
    "vti": "latest",
    "vue": "2.7",
    "vue-template-compiler": "2.7",
    "vue-tsc": "latest",
    "zhlint": "latest"
  },
  "lint-staged": {
    "{src,__tests__}/**.{js,ts}": "rome check",
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
