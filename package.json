{
  "name": "kikimore",
  "version": "0.4.16",
  "description": "趁手小型组件库",
  "author": "cloydlau",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/kikimore"
  },
  "main": "./dist/kikimore.umd.js",
  "module": "./dist/kikimore.es.js",
  "exports": {
    ".": {
      "import": "./dist/kikimore.es.js",
      "require": "./dist/kikimore.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "rd /s /q .\\node_modules && pnpm i",
    "release": "npm version patch -m 'v%s' && npm publish && git push --tags",
    "release:minor": "npm version minor -m 'v%s' && npm publish && git push --tags",
    "release:major": "npm version major -m 'v%s' && npm publish && git push --tags"
  },
  "dependencies": {
    "kayran": "latest",
    "lodash-es": "latest",
    "overlayscrollbars": "latest",
    "pic-viewer": "latest",
    "uuid": "latest",
    "viewerjs": "latest"
  },
  "peerDependencies": {
    "element-ui": "2",
    "sweetalert2": "11"
  },
  "devDependencies": {
    "@types/overlayscrollbars": "latest",
    "@vue/composition-api": "latest",
    "element-ui": "latest",
    "json-editor-vue": "latest",
    "sass": "latest",
    "svelte-jsoneditor": "latest",
    "sweetalert2": "latest",
    "typescript": "latest",
    "unocss": "latest",
    "vite": "latest",
    "vite-plugin-vue2": "latest",
    "vite-plugin-windicss": "latest",
    "vue": "2.6.12",
    "vue-template-compiler": "2.6.12"
  }
}
