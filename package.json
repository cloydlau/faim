{
  "name": "kikimore",
  "PascalCasedName": "Kikimore",
  "version": "0.14.0-beta.1",
  "private": false,
  "description": "Element Plus (Vue 3) & Element UI (Vue 2.7/2.6) all-in-one universal component library.",
  "author": "Cloyd Lau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/kikimore",
  "keywords": [
    "carousel",
    "crop",
    "cropper",
    "dropzone",
    "el-upload",
    "filepond",
    "gallery",
    "img",
    "image",
    "masonry",
    "qrcode",
    "qr code",
    "sortable",
    "swiper",
    "upload",
    "uploader",
    "viewer",
    "vue-demi",
    "waterfall"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./*": "./*"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "unpkg": "./src/index.ts",
  "jsdelivr": "./src/index.ts",
  "types": "./src/index.d.ts",
  "files": [
    "docs",
    "scripts/postinstall.mts",
    "src"
  ],
  "scripts": {
    "dev": "esno ./scripts/dev.mts",
    "build": "vite build",
    "serve": "vite preview",
    "release": "esno ./scripts/release.mts",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx only-allow pnpm",
    "postinstall": "esno ./scripts/postinstall.mts"
  },
  "peerDependencies": {
    "element-plus": ">=2",
    "element-ui": ">=2",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    },
    "element-ui": {
      "optional": true
    }
  },
  "dependencies": {
    "@vueuse/core": "latest",
    "await-to-js": "latest",
    "axios": "latest",
    "axios-shortcut": "latest",
    "cropperjs": "latest",
    "cross-spawn": "latest",
    "del": "latest",
    "esno": "latest",
    "kolorist": "latest",
    "lodash-es": "latest",
    "magicast": "latest",
    "qrcode": "latest",
    "sass": "latest",
    "sortablejs": "latest",
    "sweetalert2": "latest",
    "swiper": "latest",
    "upng-js": "latest",
    "validator": "latest",
    "viewerjs": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~0.42.1",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@types/validator": "latest",
    "@vue/composition-api": "latest",
    "@vue/test-utils": "legacy",
    "case-police": "latest",
    "element-plus": "latest",
    "element-ui": "latest",
    "eslint": "latest",
    "json-editor-vue": "latest",
    "kikimore": "workspace:*",
    "lint-staged": "latest",
    "prompts": "latest",
    "semver": "latest",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "unplugin-auto-import": "latest",
    "unplugin-vue-components": "latest",
    "unplugin-vue2-script-setup": "latest",
    "vanilla-jsoneditor": "latest",
    "vite": "latest",
    "vite-plugin-css-injected-by-js": "latest",
    "vite-plugin-dts": "latest",
    "vite-plugin-vue2": "latest",
    "vue": "~2.6.14",
    "vue-template-compiler": "~2.6.14",
    "vue-tsc": "latest",
    "zhlint": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ],
    "*": "eslint --cache --ignore-pattern 'demo/' --fix"
  }
}