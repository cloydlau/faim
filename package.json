{
  "name": "kikimore",
  "pascalCasedName": "Kikimore",
  "version": "0.10.7",
  "private": false,
  "description": "Several components wrapper for element-ui.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/kikimore",
  "keywords": [
    "vue",
    "element",
    "element-ui",
    "element-plus"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/kikimore.umd.js",
      "import": "./dist/kikimore.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/kikimore.umd.js",
  "module": "./dist/kikimore.mjs",
  "unpkg": "./dist/kikimore.umd.js",
  "jsdelivr": "./dist/kikimore.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "release": "npx case-police --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-ui": ">=2",
    "vue": "2"
  },
  "dependencies": {
    "lodash-es": "latest",
    "uuid": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-vue2": "latest",
    "case-police": "latest",
    "element-ui": "latest",
    "eslint": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "sass": "latest",
    "typescript": "latest",
    "unocss": "latest",
    "unplugin-auto-import": "latest",
    "vite": "latest",
    "vite-plugin-css-injected-by-js": "latest",
    "vite-plugin-dts": "latest",
    "vue": "^2.7.14"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}