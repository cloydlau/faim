{
  "name": "kikimore",
  "PascalCasedName": "Kikimore",
  "version": "0.13.0",
  "private": false,
  "packageManager": "pnpm@7.22.0",
  "description": "Several components wrapper for Element Plus / Element.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/kikimore",
  "keywords": [
    "vue",
    "vue-demi",
    "element",
    "element-ui",
    "element-plus",
    "dropzone",
    "filepond",
    "viewerjs",
    "qrcode",
    "swiper",
    "gallery",
    "carousel",
    "masonry",
    "waterfall",
    "map",
    "AMap",
    "TMap",
    "coordinates"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": "./*"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "unpkg": "./src/index.ts",
  "jsdelivr": "./src/index.ts",
  "types": "./src/index.d.ts",
  "files": [
    "docs",
    "src"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "build": "vite build",
    "release": "npx case-police --fix && npx zhlint README.md --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-plus": ">=2",
    "element-ui": ">=2",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    },
    "element-ui": {
      "optional": true
    }
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@tarekraafat/autocomplete.js": "^9.1.1",
    "@vueuse/core": "^9.7.0",
    "await-to-js": "^3.0.0",
    "cropperjs": "^1.5.13",
    "lodash-es": "^4.17.21",
    "qrcode": "^1.5.1",
    "sortablejs": "^1.15.0",
    "sweetalert2": "^11.6.15",
    "sweetalert2-preset": "^0.2.2",
    "swiper": "^8.4.5",
    "validator": "^13.7.0",
    "viewerjs": "^1.11.1",
    "vue-demi": "^0.13.11",
    "vue-global-config": "^0.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@iconify/json": "^2.1.153",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/validator": "^13.7.10",
    "@vitejs/plugin-vue2": "latest",
    "@vue/test-utils": "1",
    "axios": "^1.2.1",
    "axios-shortcut": "^0.1.1",
    "case-police": "^0.5.10",
    "element-ui": "latest",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "json-editor-vue": "^0.10.5",
    "lint-staged": "^13.1.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vanilla-jsoneditor": "^0.11.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vue": "2.7",
    "vue-template-compiler": "2.7",
    "zhlint": "^0.7.1"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
