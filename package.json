{
  "name": "kikimore",
  "PascalCasedName": "Kikimore",
  "version": "0.13.2",
  "private": false,
  "packageManager": "pnpm@7.25.0",
  "description": "Several components wrapper for Element Plus / Element.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/kikimore",
  "keywords": [
    "vue",
    "vue-demi",
    "element",
    "element-ui",
    "element-plus"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": "./*"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "unpkg": "./src/index.ts",
  "jsdelivr": "./src/index.ts",
  "types": "./src/index.d.ts",
  "files": [
    "docs",
    "src"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "build": "vite build",
    "release": "npx case-police --fix && npx zhlint README.md --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-plus": ">=2",
    "element-ui": ">=2",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    },
    "element-ui": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue-demi": "^0.13.11",
    "vue-global-config": "^0.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@element-plus/icons-vue": "^2.0.10",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.7",
    "case-police": "^0.5.10",
    "element-plus": "^2.2.28",
    "eslint": "^8.32.0",
    "husky": "^8.0.3",
    "json-editor-vue": "^0.10.5",
    "lint-staged": "^13.1.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vanilla-jsoneditor": "^0.11.8",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45",
    "zhlint": "^0.7.1"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
