{
  "name": "kikimore",
  "version": "0.7.0",
  "private": false,
  "description": "趁手小型组件库",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/kikimore",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/kikimore.umd.js",
      "import": "./dist/kikimore.mjs",
      "browser": "./dist/kikimore.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/kikimore.umd.js",
  "module": "./dist/kikimore.mjs",
  "unpkg": "./dist/kikimore.umd.js",
  "jsdelivr": "./dist/kikimore.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "release": "npx case-police --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-ui": ">=2",
    "vue": "2"
  },
  "dependencies": {
    "@vueuse/core": "latest",
    "lodash-es": "latest",
    "pic-viewer": "latest",
    "sweetalert2": "latest",
    "sweetalert2-preset": "latest",
    "uuid": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@vitejs/plugin-vue2": "latest",
    "case-police": "latest",
    "element-ui": "latest",
    "eslint": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "sass": "latest",
    "typescript": "latest",
    "unocss": "0.43.1",
    "unplugin-auto-import": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vue": "^2.7.10"
  },
  "lint-staged": {
    "!(demo/**/*)*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
