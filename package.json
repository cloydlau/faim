{
  "name": "kikimore",
  "PascalCasedName": "Kikimore",
  "version": "0.12.1",
  "private": false,
  "packageManager": "pnpm@7.21.0",
  "description": "Several components wrapper for element-ui.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/kikimore",
  "keywords": [
    "vue",
    "element",
    "element-ui",
    "element-plus"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/kikimore.umd.js",
      "import": "./dist/kikimore.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/kikimore.umd.js",
  "module": "./dist/kikimore.mjs",
  "unpkg": "./dist/kikimore.umd.js",
  "jsdelivr": "./dist/kikimore.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "release": "npx case-police --fix && npx zhlint README.md --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-ui": ">=2",
    "vue": "2"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.0",
    "vue-global-config": "^0.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue2": "^2.2.0",
    "case-police": "^0.5.10",
    "element-ui": "^2.15.12",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "unocss": "^0.47.6",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.3",
    "vite-plugin-css-injected-by-js": "^2.2.0",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^2.7.14",
    "zhlint": "^0.7.1"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}